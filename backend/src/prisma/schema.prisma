generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Dog {
  id          Int      @id @default(autoincrement())
  name        String
  breed       String
  age         Int
  gender      String
  location    String
  description String
  imageUrl    String?
  createdAt   DateTime @default(now())
}
