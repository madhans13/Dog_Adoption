generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Dog {
  id          Int      @id @default(autoincrement())
  name        String
  breed       String
  age         Int
  gender      String
  location    String
  description String
  imageUrl    String?
  createdAt   DateTime @default(now())

  @@map("Dog")
}